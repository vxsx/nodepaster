{
  "name": "nodepaster",
  "version": "0.3.0",
  "description": "Cli tool for pasting to dpaste.de",
  "main": "index.es5.js",
  "bin": {
    "dpaster": "cli.es5.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "transpile": "babel cli.js > cli.es5.js && babel index.js > index.es5.js",
